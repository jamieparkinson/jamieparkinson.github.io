{
  "name": "jamieparkinson.github.io",
  "version": "1.0.0",
  "description": "My website",
  "main": "index.js",
  "scripts": {
    "build-task:scss-compile": "node-sass-chokidar --source-map true scss/ -o css/",
    "build-task:autoprefixer": "postcss css/*.css --use autoprefixer -d css/",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'scss/**/*.scss' -c 'npm run sass:build'",
    "dev": "npm-run-all -p sass:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamieparkinson/jamieparkinson.github.io.git"
  },
  "author": "Jamie Parkinson <jamie@drfocused.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamieparkinson/jamieparkinson.github.io/issues"
  },
  "homepage": "https://github.com/jamieparkinson/jamieparkinson.github.io#readme",
  "dependencies": {
    "autoprefixer": "^7.1.1",
    "chokidar-cli": "^1.2.0",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.0.2",
    "postcss-cli": "^4.1.0"
  }
}
